    "analytics.js": "^2.9.1",
    "apollo-boost": "^0.4.4",
    "axios": "^0.19.2",
    "clsx": "^1.0.4",
    "firebase": "^7.5.2",
    "formik": "^2.1.1",
    "graphql": "^14.5.8",
    "moment": "^2.24.0",
    "react": "^16.12.0",
    "react-beautiful-dnd": "^11.0.5",
    "react-dom": "^16.12.0",
    "react-linkify": "^1.0.0-alpha",
    "react-modal": "^3.11.1",

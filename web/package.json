    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "2.x",
    "firebase-tools": "^7.8.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.4",
    "postcss-cli": "^6.1.3",
    "prettier": "^2.0.5",
    "tailwindcss": "^1.1.3",
    "typescript": "3.7.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
